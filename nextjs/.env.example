# Presenton Frontend Configuration
# Copy this file to .env.local

# Disable frontend configuration (required for backend-managed mode)
CAN_CHANGE_KEYS=false

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# Optional: User config path (not used when CAN_CHANGE_KEYS=false)
USER_CONFIG_PATH=/tmp/presenton/user_config.json
